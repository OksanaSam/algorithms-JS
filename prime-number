     // Check if a number is prime
     //https://whatis.techtarget.com/definition/prime-number
     //////////////////////////////////////////////////////
     
     function isPrime(num) {
        if (num === 0 || num === 1) {
          return false;
        }
        for (let i = 2; i < num; i++) {
          if (num % i === 0) {
            return false;
          }
        }
        return true;
      }

      isPrime(10);
